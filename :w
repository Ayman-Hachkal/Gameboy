import numpy as np

class Registers:
    def __init__(self) -> None:
        self.register = {
            'a':np.uint8(),
            'b':np.uint8(),
            'c':np.uint8(),
            'd':np.uint8(),
            'e':np.uint8(),
            'f':np.uint8(),
            'h':np.uint8(),
            'l':np.uint8(),
        }
    def get_bc(self, bc):
        b = np.uint16((self.register['b'] & 0xff00) >> 8)
        c = np.uint16((self.register['c'] & 0xff))

        return np.uint16

